<div class="container section scrollspy" id="themes">
  <div id="overflowTest1">
    <div class="row">
      <form action="/dealer/add" method="POST">
        <div class="row">
          <div class="input-field col s4">
            <input name="carId" id="tbNumbers" type="text" class="validate" required
              onkeypress="javascript:return isNumber(event)">
            <label for="carId">id</label>
          </div>
          <div class="input-field col s4">
            <input name="make" id="make" type="text" class="validate" required>
            <label for="make">Make</label>
          </div>
          <div class="input-field col s4">
            <input name="model" id="model" type="text" class="validate" required>
            <label for="model">Model</label>
          </div>

        </div>

        <div class="row">
          <div class="input-field col s3">
            <input name="engDscr" id="engDscr" type="text" class="validate" required>
            <label for="engDscr">eng dscr</label>
          </div>

          <div class="input-field col s9">
            <input name="photo" id="photo" type="text" class="validate" required>
            <label for="photo">Photo</label>
          </div>
        </div>

        <div class="row">
          <div class="input-field col s4">
            <input name="fuelType1" id="fuelType1" type="text" class="validate" required>
            <label for="fuelType1">fuelType1</label>
          </div>

          <div class="input-field col s4">
            <input name="VClass" id="VClass" type="text" class="validate" required>
            <label for="VClass">VClass</label>
          </div>

          <div class="input-field col s4">
            <input name="trany" id="trany" type="text" class="validate" required>
            <label for="trany">trany</label>
          </div>
        </div>

        <div class="row">
          <div class="input-field col s6">
            <input name="createdOn" id="createdOn" type="text" class="validate" required>
            <label for="createdOn">createdOn</label>
          </div>

          <div class="input-field col s6">
            <input name="fuelCost08" id="tbNumbers" type="text" class="validate" required
              onkeypress="javascript:return isNumber(event)">
            <label for="fuelCost08">fuelCost08</label>
          </div>
        </div>
        <button class="waves-effect waves-light btn" type="submit" onclick="myFunction()">Add car</button>
      </form>
    </div>

  </div>
</div>



