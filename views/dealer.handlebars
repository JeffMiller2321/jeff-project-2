

<div class="container section scrollspy" id="themes">
  <div id="overflowTest">
<div class="row">
  <form action="/dealer/add" method="POST">
    <div class="row">
      <div class="input-field col s6">
        <input name="carId" id="carId" type="text" class="validate">
        <label for="carId">id</label>
      </div>
      <div class="input-field col s6">
        <input name="make" id="make" type="text" class="validate">
        <label for="make">Make</label>
      </div>
      <div class="input-field col s6">
        <input name="model" id="model" type="text" class="validate">
        <label for="model">Model</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <input name="engDscr" id="engDscr" type="text" class="validate">
        <label for="engDscr">eng dscr</label>
      </div>

      <div class="input-field col s6">
        <input name="photo" id="photo" type="text" class="validate">
        <label for="photo">Photo</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <input name="fuelType1" id="fuelType1" type="text" class="validate">
        <label for="fuelType1">fuelType1</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <input name="VClass" id="VClass" type="text" class="validate">
        <label for="VClass">VClass</label>
      </div>

      <div class="input-field col s6">
        <input name="trany" id="trany" type="text" class="validate">
        <label for="trany">trany</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <input name="createdOn" id="createdOn" type="text" class="validate">
        <label for="createdOn">createdOn</label>
      </div>

      <div class="input-field col s6">
        <input name="fuelCost08" id="fuelCost08" type="text" class="validate">
        <label for="fuelCost08">fuelCost08</label>
      </div>
    </div>
 <button class="waves-effect waves-light btn" type="submit">Add car</button>
  </form>
</div>

</div>
</div>


 
